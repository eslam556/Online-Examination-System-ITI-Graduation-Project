                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	S.Student_ID AS StudentID,
	SC.Course_ID AS CourseID,
	NULL AS CertificateID,
	NULL AS EmploymentID,
	S.Birth_Date AS DateID,
	'Grade' AS RecordType,
	SC.Grade AS GradeValue
FROM
	Student S
JOIN
	Student_Course SC
ON
	S.Student_ID = SC.Student_ID

UNION ALL

SELECT DISTINCT
	S.Student_ID AS StudentID,
	NULL AS CourseID,
	C.Certificate_ID AS CertificateID,
	NULL AS EmploymentID,
	NULL AS DateID,
	'Certification' AS RecordType,
	NULL AS GradeValue
FROM
	Student S
JOIN
	Student_Course SC
ON
	S.Student_ID = SC.Student_ID
JOIN
	Student_Certificate C
ON
	C.Student_ID = S.Student_ID

UNION ALL

SELECT DISTINCT
	S.Student_ID AS StudentID,
	NULL AS CourseID,
	NULL AS CertificateID,
	H.Hiring_ID AS EmploymentID,
	H.Hiring_Date AS DateID,
	'Employment' AS RecordType,
	NULL AS GradeValue
FROM
	Student S
JOIN
	Student_Course SC
ON
	S.Student_ID = SC.Student_ID
JOIN
	Hiring H
ON
	H.Student_ID = S.Student_ID
ORDER BY
	S.Student_ID</property>
