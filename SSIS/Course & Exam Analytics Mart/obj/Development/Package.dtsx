                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH StudentTotalMarks AS
(SELECT
	G.exam_id,
	S.Student_ID,
	SUM(S.Student_Answer_Mark) AS TotalMarks
FROM
	generated_exams G
JOIN
	exam_questions Q
ON
	G.exam_id = Q.exam_id
JOIN
	Student_Answer S
ON
	S.Question_ID = Q.question_id
GROUP BY
	G.exam_id,
	S.Student_ID
),
PassedStudents AS
(
	SELECT
		STM.exam_id,
		COUNT(*) AS PassedCount
	FROM
		StudentTotalMarks STM
	JOIN
		generated_exams G
	ON
		STM.exam_id = G.exam_id
	WHERE
		STM.TotalMarks &gt;= 0.5 * G.total_mark
	GROUP BY
		STM.exam_id
),
TotalStudents AS
(
	SELECT
		exam_id,
		COUNT(*) AS TotalCount
	FROM
		StudentTotalMarks
	GROUP BY
		exam_id
)

SELECT DISTINCT
	C.Course_ID,
	G.exam_id,
	Q.Question_ID,
	T.Topic_ID,
	ISNULL(CAST(P.PassedCount * 100.0 / TS.TotalCount AS decimal(5,2)), 0) AS PassRate
FROM
    Course C
JOIN
	Topic T ON C.Course_ID = T.Course_ID
JOIN
    generated_exams G ON G.course_id = C.Course_ID
JOIN
    TotalStudents TS ON TS.exam_id = G.exam_id
LEFT JOIN
    PassedStudents P ON P.exam_id = G.exam_id
JOIN
	exam_questions Q
ON
	Q.exam_id = G.exam_id
ORDER BY
	C.Course_ID,
	G.exam_id,
	Q.question_id</property>
